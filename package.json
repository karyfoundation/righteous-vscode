{
	"name": "righteous",
	"displayName": "Righteous",
	"description": "Kary Foundation Coding Style's Code Formatter for TypeScript and JavaScript",
	"version": "2.0.1",
	"publisher": "karyfoundation",
	"engines": {
		"vscode": "^0.10.1"
	},
	"icon": "icon.png",
	"categories": [
		"Other"
	],
	"main": "./out/src/extension",
	"activationEvents": [
		"onLanguage:typescript",
		"onLanguage:javascript",
		"onLanguage:css"
	],
	"scripts": {
		"vscode:prepublish": "node ./node_modules/vscode/bin/compile",
		"compile": "node ./node_modules/vscode/bin/compile -watch -p ./"
	},
	"devDependencies": {
		"typescript": "^1.6.2",
		"vscode": "0.10.x"
	},
	"dependencies": {
		"righteous-core": "^2.0.20"
	},
